<h1> Sign Up </h1>
<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %> 
  <br> 
  <%= f.label :password %>
  <%= f.text_field :password %>
  <br>  
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br> 
  <%= f.submit 'Sign Up' %>
<% end %>